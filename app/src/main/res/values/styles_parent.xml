        <item name="android:textSelectHandle">@drawable/cabinet_text_select_handle_middle</item>
        <item name="android:actionModeCloseDrawable">@drawable/ic_action_close</item>
        <item name="android:popupMenuStyle">@style/Theme.PopupMenu</item>
        <item name="android:dropDownListViewStyle">
            @android:style/Widget.Holo.Light.ListView.DropDown
        </item>
        <item name="android:actionBarStyle">@style/ActionBar.Solid.Cabinet</item>
        <item name="android:actionModeBackground">@drawable/ab_solid_cabinet</item>
        <item name="android:actionBarWidgetTheme">@style/Theme.Cabinet.Widget</item>
        <item name="android:actionBarSize">56dp</item>
        <item name="android:actionOverflowButtonStyle">@style/OverflowRecoup</item>
        <item name="android:dialogTheme">@android:style/Theme.Holo.Light.Dialog</item>

        <item name="android:itemTextAppearance">@style/ActionMenuTextAppearance</item>
        <item name="android:homeAsUpIndicator">@drawable/ic_action_arrow_back</item>
    </style>

    <style name="ActionMenuTextAppearance">
        <item name="android:textColor">@android:color/black</item>
    </style>

